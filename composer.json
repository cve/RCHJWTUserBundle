{
    "name": "chalasr/jwt-user-bundle",
    "description": "Json Web Token authentication bundle including FOSUserBundle integration",
    "keywords": ["jwt", "authentication", "user", "token", "rest"],
    "type": "symfony-bundle",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Robin CHALAS",
            "email": "robin.chalas@gmail.com",
            "homepage": "https://github.com/chalasr/RCHJWTUserBundle"
        }
    ],
    "require": {
        "php": ">= 5.3.9",
        "symfony/framework-bundle": "~2.4|~3.0",
        "symfony/security-bundle": "~2.4|~3.0",
        "symfony/http-foundation": "~2.4|~3.0",
        "symfony/http-kernel": "~2.4|~3.0",
        "symfony/dependency-injection": "~2.4|~3.0",
        "symfony/doctrine-bridge": "~2.4|~3.0",
        "symfony/console": "~2.4|~3.0",
        "symfony/process": "~2.4|~3.0",
        "symfony/filesystem": "~2.4|~3.0",
        "symfony/yaml": "~2.4|~3.0",
        "friendsofsymfony/user-bundle": "~1.3",
        "lexik/jwt-authentication-bundle": "~1.3.1",
        "gesdinet/jwt-refresh-token-bundle": "~0.1",
        "fabpot/goutte": "~2.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": { "RCH\\JWTUserBundle\\": "" }
    },
    "scripts": {
        "post-install-cmd": [
            "php app/console rch:jwt:generate-keys"
        ],
        "post-update-cmd": [
            "php app/console rch:jwt:generate-keys"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
