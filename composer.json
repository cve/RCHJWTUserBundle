{
    "name": "rch/jwt-user-bundle",
    "description": "Json Web Token authentication bundle including FOSUserBundle integration",
    "keywords": ["jwt", "authentication", "user", "token", "rest"],
    "type": "symfony-bundle",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Robin CHALAS",
            "email": "robin.chalas@gmail.com",
            "homepage": "https://github.com/chalasr/RCHJWTUserBundle"
        }
    ],
    "require": {
        "php": ">= 5.4.0",
        "symfony/symfony": "~2.8|~3.0",
        "friendsofsymfony/user-bundle": "~1.3",
        "fabpot/goutte": "~1.0|~2.0|~3.0",
        "lexik/jwt-authentication-bundle": "~1.0",
        "gesdinet/jwt-refresh-token-bundle": "~0.1",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "symfony/swiftmailer-bundle": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "RCH\\JWTUserBundle\\": ""
         }
    },
    "extra": {
        "branch-alias": {
            "1.x": "1.2.x-dev",
            "dev-master": "2.0.x-dev"
        }
    }
}
