{
    "name": "rch/jwt-user-bundle",
    "description": "Json Web Token authentication bundle including FOSUserBundle integration",
    "keywords": ["jwt", "authentication", "user", "token", "rest"],
    "type": "symfony-bundle",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Robin CHALAS",
            "email": "robin.chalas@gmail.com",
            "homepage": "https://github.com/chalasr/RCHJWTUserBundle"
        }
    ],
    "require": {
        "php": ">= 5.4.0",
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/security-bundle": "~2.8|~3.0",
        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/doctrine-bridge": "~2.8|~3.0",
        "symfony/console": "~2.8|~3.0",
        "symfony/process": "~2.8|~3.0",
        "symfony/filesystem": "~2.8|~3.0",
        "symfony/yaml": "~2.8|~3.0",
        "friendsofsymfony/user-bundle": "~1.3|~2.0",
        "fabpot/goutte": "~1.0|~2.0|~3.0",
        "lexik/jwt-authentication-bundle": "~1.0",
        "gesdinet/jwt-refresh-token-bundle": "~0.1"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": { "RCH\\JWTUserBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
